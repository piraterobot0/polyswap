# Example environment configuration
# Copy this file to .env and fill in your values
# NEVER commit .env file with real values to version control!

# Deployer wallet private key (without 0x prefix)
# CRITICAL: Use a secure key management solution in production
PRIVATE_KEY="your_private_key_here_never_commit_real_key"

# Gas price in gwei for deployments
GAS_PRICE='25'

# Polygonscan API key for contract verification
# Get one at https://polygonscan.com/apis
POLYGONSCAN_API_KEY="your_polygonscan_api_key"

# Infura/Alchemy API key for RPC access
METAMASK_API_KEY="your_api_key_here"

# Polygon RPC URL
POLYGON_INFURA="https://polygon-mainnet.infura.io/v3/your_api_key_here"

# Uniswap V4 Contract Addresses (Polygon Mainnet)
POOLMANAGER=0x67366782805870060151383F4BbFF9daB53e5cD6
